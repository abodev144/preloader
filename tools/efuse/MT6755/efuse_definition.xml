<efuse_writer chip="6755" output_bin_size="512">
	<definitions>
		<!-- Inner Value declaration-->
		<inner_value tag="magic-key" attribute="key1">
			<require length="8" />
			<suppress_log />
		</inner_value>
		<inner_value tag="magic-key" attribute="key2">
			<require length="8" />
			<suppress_log />
		</inner_value>
		<inner_value tag="usb-id" attribute="vid">
